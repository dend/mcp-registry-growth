# Development Environment Configuration for MCP Registry Growth Dashboard
# Copy this file to .env.local and customize as needed

# Build Configuration
NODE_ENV=development

# Local Development Configuration
NEXT_PUBLIC_BASE_PATH=
NEXT_PUBLIC_ASSET_PREFIX=

# Performance Configuration
NEXT_PUBLIC_CACHE_DURATION=60000

# Feature Flags
NEXT_PUBLIC_ENABLE_MOCK_DATA=true
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true

# Data Source Configuration
NEXT_PUBLIC_DATA_SOURCE=static
NEXT_PUBLIC_DATA_UPDATE_INTERVAL=300000

# Optional: If you want to test with real MCP Registry API
# NEXT_PUBLIC_MCP_REGISTRY_API=https://api.modelcontextprotocol.org
# NEXT_PUBLIC_API_KEY=your_api_key_here